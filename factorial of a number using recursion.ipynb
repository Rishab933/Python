   "source": [
    "n = int(input(\"Enter the number : \"))\n",
    "def fact(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        p = n * fact(n-1)\n",
    "        return p\n",
    "\n",
    "output = fact(n)\n",
    "print(\"output is :\",output)"
   ]
  }
