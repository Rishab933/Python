   "source": [
    "n = int(input(\"Enter the number of terms : \"))\n",
    "def fibo(n):\n",
    "    if n == 1:\n",
    "        return 0\n",
    "    elif n == 2:\n",
    "        return 1\n",
    "    else:\n",
    "        return (fibo(n-1)+fibo(n-2))\n",
    "\n",
    "print(fibo(n))"
   ]
  },
